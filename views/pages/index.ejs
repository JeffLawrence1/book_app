<!DOCTYPE html>
<html lang="en">
<%- include('../layout/head')%>
<body>
  <main>
    
    <h1>Current Books in Database</h1>
    <a href="/searches/new"><button>Search Books</button></a>
    <h3>Book Count = <%=bookCount%></h3>
    <% result.forEach(results => { %>
    <%- include('./books/detail', {book: results}) %>
    <a href="/books/<%= results.id %>"><button class="viewBut">View Book Details</button></a>
    <% }) %>
  </main>
</body>
</html>
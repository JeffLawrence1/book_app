<!DOCTYPE html>
<html lang="en">
<%- include('../../layout/head')%>
<body>
  <main id="bookSearchSection">
    <h1>Book Search Results</h1>
    <a href="/"><button>To Library</button></a>
      <% searchResults.forEach(book => { %>
        <section>
        <%- include('../books/detail', {book: book}) %>
         <button class="viewBut">Select Book</button>
         <%- include('../books/edit', { element: book, formAction: 'Add' }) %>
        </section>
      <% }) %>
</main>
<script
  src="https://code.jquery.com/jquery-3.4.1.js"
  integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
  crossorigin="anonymous"></script>
  <script src="/js/app.js"></script>
</body>
</html>